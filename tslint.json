{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "object-literal-sort-keys": false,
    "interface-over-type-literal": false,
    "no-consecutive-blank-lines": [true, 2],
    "member-ordering": [true, {"order": "instance-sandwich"}],
    "trailing-comma": [
      true,
      {
        "multiline": "always",
        "singleline": "never"
      }
    ]
  }
}